#方法一： 暴力求解，会超时
class Solution:
    def maxProfit(self, prices: List[int]) -> int:
        maxp = 0
        inday = 0
        outday = 1
        length = len(prices)
        for i in range(length-1):
            for j in range(i+1, length):
                if(maxp <= prices[j] - prices[i]):
                    inday = i
                    outday = j
                    maxp = prices[j] - prices[i]
        return maxp
